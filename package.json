{
	"name": "react-amalgamation",
	"version": "0.0.0",
	"scripts": {
		"format": "prettier 'src/**/*.ts'",
		"format:fix": "prettier --write 'src/**/*.ts'",
		"lint": "eslint 'src/**/*.ts'",
		"lint:fix": "eslint --fix 'src/**/*.ts'",
